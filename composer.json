{
    "name": "ns/distance-bundle",
    "type": "symfony-bundle",
    "description": "A way to handle postal code distance calculations",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        }
    ],
    "require": {
        "php":                    ">=7.2",
        "kzykhys/php-csv-parser": "^1.4",
        "symfony/http-client":    "^4.3",
        "doctrine/orm":           "^2.0"
    },
    "autoload": {
        "psr-4": { "NS\\DistanceBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NS\\DistanceBundle\\Tests\\": "Tests" }
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0|^6.0",
        "liip/rmt": "1.*"
    }
}
