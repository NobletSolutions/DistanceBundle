{
    "name": "ns/distance-bundle",
    "type": "symfony-bundle",
    "description": "A way to handle postal code distance calculations",
    "license": "MIT",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        }
    ],
    "require": {
        "php":              ">=5.3.2",
	"symfony/symfony":  "^2.7|^3.0"
    },
    "autoload": {
        "psr-4": { "NS\\DistanceBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "NS\\DistanceBundle\\Tests\\": "Tests" }
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "liip/rmt": "1.*"
    }
}
